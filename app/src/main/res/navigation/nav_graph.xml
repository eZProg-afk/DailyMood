<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/registrationFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.main.view.MainFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/toCreateEmotion"
            app:destination="@id/createEmotionFragment" />
        <action
            android:id="@+id/toDetail"
            app:destination="@id/detailEmotionFragment" />
        <action
            android:id="@+id/toSelect"
            app:destination="@id/selectionTypeCreatingEmotionFragment" />
    </fragment>
    <fragment
        android:id="@+id/createEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.create.view.CreateEmotionFragment"
        android:label="@string/create_emotion_label"
        tools:layout="@layout/fragment_create_emotion">
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/detailEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.detail.view.DetailEmotionFragment"
        android:label="@string/detail_emotion_label"
        tools:layout="@layout/fragment_detail_emotion">
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
        <argument
            android:name="emotionId"
            app:argType="long" />
    </fragment>
    <fragment
        android:id="@+id/registrationFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.registration.view.RegistrationFragment"
        android:label="@string/registration_label"
        tools:layout="@layout/fragment_registration">
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/toEmailRegistration"
            app:destination="@id/emailRegFragment" />
        <action
            android:id="@+id/toEmailAuthenticate"
            app:destination="@id/emailAuthFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailAuthFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.registration.email_auth.view.EmailAuthFragment"
        android:label="@string/email_auth_label"
        tools:layout="@layout/fragment_email_auth">
        <action
            android:id="@+id/toEmailRegistration"
            app:destination="@id/emailRegFragment" />
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/emailRegFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.registration.email_reg.view.EmailRegFragment"
        android:label="@string/email_reg_label"
        tools:layout="@layout/fragment_email_reg">
        <action
            android:id="@+id/toEmailAuthenticate"
            app:destination="@id/emailAuthFragment" />
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/statisticFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.statistic.view.StatisticFragment"
        android:label="fragment_statistic"
        tools:layout="@layout/fragment_statistic">
        <action
            android:id="@+id/toDetail"
            app:destination="@id/detailEmotionFragment" />
    </fragment>
    <fragment
        android:id="@+id/selectionTypeCreatingEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.select_creation_type.view.SelectionTypeCreatingEmotionFragment"
        android:label="SelectionTypeCreatingEmotionFragment"
        tools:layout="@layout/fragment_selection_add_emotion_type">
        <action
            android:id="@+id/toManually"
            app:destination="@id/manuallyAddEmotionFragment" />
        <action
            android:id="@+id/toSurvey"
            app:destination="@id/surveyAddEmotionFragment" />
        <action
            android:id="@+id/toVoice"
            app:destination="@id/voiceAddEmotionFragment" />
        <action
            android:id="@+id/toPhoto"
            app:destination="@id/photoAddEmotionFragment" />
    </fragment>
    <fragment
        android:id="@+id/manuallyAddEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.manually_add_mood.view.EmotionCreationByPhotoManuallyFragment"
        android:label="fragment_manually_add_emotion"
        tools:layout="@layout/fragment_emotion_creation_manually" />
    <fragment
        android:id="@+id/photoAddEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.photo_add_mood.view.EmotionCreationByPhotoFragment"
        android:label="fragment_photo_add_emotion"
        tools:layout="@layout/fragment_emotion_creation_by_photo">
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
        <action
            android:id="@+id/toEmotionCreationByRealtime"
            app:destination="@id/emotionCreationByRealtimeFragment" />
        <action
            android:id="@+id/toEmotionCreationByCamera"
            app:destination="@id/emotionCreationByCameraFragment" />
        <action
            android:id="@+id/toShowMoodRatingCameraFragment"
            app:destination="@id/showMoodRatingCameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/surveyAddEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.survey_add_mood.view.EmotionCreationBySurveyFragment"
        android:label="fragment_survey_add_emotion"
        tools:layout="@layout/fragment_emotion_creation_by_survey" >
        <action
            android:id="@+id/toSurveyResultMoodFragment"
            app:destination="@id/surveyResultMoodFragment" />
    </fragment>
    <fragment
        android:id="@+id/voiceAddEmotionFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.voice_add_mood.view.VoiceAddEmotionFragment"
        android:label="fragment_voice_add_emotion"
        tools:layout="@layout/fragment_voice_add_emotion">
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/emotionCreationByRealtimeFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.realtime_add_mood.view.EmotionCreationByRealtimeFragment"
        android:label="fragment_emotion_creation_by_realtime"
        tools:layout="@layout/fragment_emotion_creation_by_realtime" >
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
    <fragment
        android:id="@+id/emotionCreationByCameraFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.camera_add_mood.view.EmotionCreationByCameraFragment"
        android:label="fragment_emotion_creation_by_camera"
        tools:layout="@layout/fragment_emotion_creation_by_camera">
        <action
            android:id="@+id/toShowMoodRatingCameraFragment"
            app:destination="@id/showMoodRatingCameraFragment" />
    </fragment>
    <fragment
        android:id="@+id/showMoodRatingCameraFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.camera_result_mood.view.ShowMoodRatingCameraFragment"
        android:label="fragment_show_mood_rating_camera"
        tools:layout="@layout/fragment_show_mood_rating_camera">
        <action
            android:id="@+id/toEmotionCreationByCameraFragment"
            app:destination="@id/emotionCreationByCameraFragment" />
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
        <argument
            android:name="smilingProbabilities"
            app:argType="float" />
        <argument
            android:name="photoPath"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/surveyResultMoodFragment"
        android:name="spiral.bit.dev.dailymood.ui.feature.survey_result_mood.view.SurveyResultMoodFragment"
        android:label="fragment_survey_result_mood"
        tools:layout="@layout/fragment_survey_result_mood" >
        <argument
            android:name="scores"
            app:argType="integer[]" />
        <action
            android:id="@+id/toSurveyAddEmotionFragment"
            app:destination="@id/surveyAddEmotionFragment" />
        <action
            android:id="@+id/toMain"
            app:destination="@id/mainFragment" />
    </fragment>
</navigation>